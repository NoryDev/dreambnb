<p id="notice"><%= notice %></p>

<h1>Listing Flat Pics</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Flat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @flat_pics.each do |flat_pic| %>
      <tr>
        <td><%= flat_pic.description %></td>
        <td><%= flat_pic.flat %></td>
        <td><%= link_to 'Show', flat_pic %></td>
        <td><%= link_to 'Edit', edit_flat_pic_path(flat_pic) %></td>
        <td><%= link_to 'Destroy', flat_pic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Flat pic', new_flat_pic_path %>
